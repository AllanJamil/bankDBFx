      <Button fx:id="withdrawDepositButton" layoutX="68.0" layoutY="153.0" mnemonicParsing="false" onAction="#okButtonAction" prefHeight="49.0" prefWidth="108.0" text="OK">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="cancelButton" layoutX="224.0" layoutY="153.0" mnemonicParsing="false" onAction="#close" prefHeight="49.0" prefWidth="108.0" text="Cancel">
