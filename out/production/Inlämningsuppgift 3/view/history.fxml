<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.HistoryController">
   <children>
      <TableView fx:id="historyTable" layoutX="90.0" layoutY="70.0" prefHeight="200.0" prefWidth="420.0">
        <columns>
          <TableColumn fx:id="dateCol" editable="false" prefWidth="126.0" text="Date" />
          <TableColumn fx:id="balanceCol" editable="false" prefWidth="124.0" text="Balance" />
            <TableColumn fx:id="transactionCol" editable="false" prefWidth="169.0" text="Transaction Amount" />
        </columns>
      </TableView>
      <Label layoutX="90.0" layoutY="39.0" text="History">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Button fx:id="okButton" layoutX="245.0" layoutY="308.0" mnemonicParsing="false" onAction="#close" prefHeight="50.0" prefWidth="111.0" text="OK">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
